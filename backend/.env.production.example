# Ambiente de Produção - Backend

# Banco de dados PostgreSQL (atualizar com credenciais reais)
DATABASE_URL="postgresql://usuario:senha@host:5432/agreste_zeladoria?schema=public"

# JWT Secret (gerar uma chave segura única)
JWT_SECRET="sua-chave-secreta-aqui-minimo-32-caracteres"

# Porta do servidor
PORT=3001

# Ambiente
NODE_ENV=production

# CORS Origins (domínio do frontend em produção)
CORS_ORIGIN=https://seu-dominio.com

# URL base da API
API_URL=https://api.seu-dominio.com
